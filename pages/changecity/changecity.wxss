page {
  background: #f7f7f7;
}

.iconsearch {
  margin-right: 28rpx;
  margin-left: 60rpx;
  width: 27rpx;
  height: 27rpx;
  background-image: url('data:image/png;base64,');
  background-repeat: no-repeat;
  background-size: 100% 100%;
}

.inputview {
  /* position: fixed; *//* z-index: 1; */
  width: 90%;
  margin-left: 5%;
  height: 56rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  margin-top: 2%;
  margin-bottom: 2%;
  background-color: #ececec;
  border-radius: 25rpx;
  font-size: 24rpx;
}

.searchLetter {
  position: fixed;
  right: 0;
  width: 70rpx;
  height: 80%;
  margin-top: 10%;
  text-align: center;
  justify-content: center;
  font-size: 28rpx;
  display: flex;
  flex-direction: column;
  z-index: 1;
  color: #007aff;
}

.selection {
  display: flex;
  width: 100%;
  /* margin-top: 26rpx; */
  flex-direction: column;
  background-color: #fff;
}

.shadows {
  height: 26rpx;
  width: 100%;
  background-color: #f7f7f7;
}

.item_letter {
  display: flex;
  height: 84rpx;
  font-size: 35rpx;
  margin-left: 52rpx;
  margin-right: 52rpx;
  align-items: center;
  border-bottom: 1rpx solid #f8f8f8;
}

.item_city {
  color: #5f5959;
  font-size: 28rpx;
  display: flex;
  background-color: #fff;
  height: 84rpx;
  padding-left: 52rpx;
  align-items: center;
}

::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}
