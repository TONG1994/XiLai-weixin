page {
  /* height: 100%; */
  background: #f7f7f7;
}

.contentcss {
  /* width: 100%; */
  background-color: #fff;
  margin-top: 24rpx;
  padding: 24rpx 32rpx 24rpx 20rpx;
}

.searchtitle {
  display: flex;
  align-items: center;
  flex-wrap: nowrap;
  /* border: 2rpx solid #979797; *//* width: 90%; */
}

.citybutton {
  display: flex;
  align-items: center;
  flex-wrap: nowrap;
  height: 50rpx;
  /* width: 114rpx; */
}

.citytext {
  max-width: 140rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  font-size: 28rpx;
  /* width: 56rpx; */
  margin-left: 20rpx;
}

.down {
  width: 28rpx;
  height: 30rpx;
  /* background-size: contain; */
  margin-left: 10rpx;
  background-image: url('data:image/png;base64,');
  background-repeat: no-repeat;
  background-size: 100% 100%;
}

.map {
  width: 32rpx;
  height: 34rpx;
  margin-left: 20rpx;
}

.vertical {
  height: 58rpx;
  width: 2rpx;
  border-right: 2rpx solid #979797;
  opacity: 0.204;
  margin-left: 23rpx;
}

.inputcss {
  width: 420rpx;
  font-size: 28rpx;
  margin-left: 24rpx;
}

scroll-view {
  width: 100%;
  margin-top: 10rpx;
}

.listcontent {
  border-top: 1rpx solid #f1f1f1;
  display: flex;
  align-items: center;
  flex-wrap: nowrap;
  padding-top: 24rpx;
  padding-bottom: 20rpx;
}

.listtext {
  font-size: 28rpx;
  color: #000;
  margin-left: 16rpx;
}
