page {
  background-color: #f7f7f7;
  height: 100%;
}

.hide-button {
  border:none !important;
}  
.sent-wrap {
  flex: 1;
  display: flex;
  align-items: center;
  height: 698rpx;
  background-image: url('data:image/png;base64,');
  background-repeat: no-repeat;
  background-size: 100% 100%;
}

.sent-btn {
  width: 412rpx;
  height: 412rpx;
  margin: 0 auto;
  background-image: url('data:image/png;base64,');
  background-repeat: no-repeat;
  background-size: 100% 100%;
}

.sent-btn .icon-wrap {
  position: relative;
  width: 208rpx;
  margin: 0 auto 24rpx auto;
  padding: 82rpx 0 16rpx 0;
}

.sent-icon {
  width: 116rpx;
  height: 116rpx;
  margin: 84rpx auto 10rpx;
  background-image: url('data:image/png;base64,');
  background-repeat: no-repeat;
  background-size: 100% 100%;
}

.sent-btn .send {
  font-size: 50rpx;
  color: #ff4747;
  letter-spacing: 10rpx;
  text-align: center;
  line-height:70rpx;
}

.sent-btn::after {
  background-image: url('data:image/png;base64,');
}

.sent-btn:active {
  background-image: url('data:image/png;base64,');
}

.sent-btn:active .sent-icon {
  background-image: url('data:image/png;base64,');
}

.record-wrap {
  flex: 1;
  display: flex;
  justify-content: center;
}

.record-wrap .record {
  display: inline-block;
  height: 76rpx;
  margin: 110rpx 0 0 0;
}

.realName-wrap {
  position: absolute;
  bottom: 30rpx;
  width: 100%;
  text-align: center;
}

.realName-wrap .realname {
  font-size: 28rpx;
  color: #a8a8a8;
}

.foot-wrap {
  position: absolute;
  bottom: 0;
  width: 100%;
}
