.box {
  background: #f2f2f2;
  width: 750rpx;
}
.box.navbar{
  padding-bottom: 100rpx;
}

.expressMess {
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 32rpx 0 34rpx 50rpx;
  margin: 25rpx 0 20rpx 0;
  background: #fff;
  font-size: 28rpx;
}

.expressMess .nextFlow{
  display: flex;
  align-items: center;
}
 .logisticFlow{
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
}

.nextFlow .companyImg {
  width: 74rpx;
  height: 74rpx;
  margin: 0 22rpx 0 0;
  background-size: contain;
}
.backgroundImg{
  padding: 10rpx;
  margin-left: 40rpx;
}
.backgroudImg-hover{
  background-color: forestgreen;
}

.copyImgFlow{
  display: flex;
  width: 44rpx;
  height: 44rpx;
}

.backgroundImg-hover{
  background-color: #e6e6e6
}

.box .expressNo {
  align-items: center;
  height: 132rpx;
  display: flex;
  justify-content: space-between;
  font-size: 30rpx;
  line-height: 70rpx;
  color: #fff;
  background-image: url('data:image/png;base64,');
}

.box .expressNo .toptext {
  margin-left: 80rpx;
}

.box .expressNo .status {
  margin: 0 26rpx 0 0;
}

.box .tabs {
  background: #fff;
  width: 750rpx;
  height: 104rpx;
  line-height: 104rpx;
  color: #121212;
  font-size: 32rpx;
  display: flex;
}

.box .tabs .tab {
  width: 375rpx;
  text-align: center;
}

.box .tabsline {
  position: relative;
  left: 0rpx;
  width: 375rpx;
  height: 2rpx;
  background: #f45322;
  margin: 0 0 16rpx 0;
  transition: 0.4s all ease;
}

.box .tabscontents .tabcontent {
  display: none;
}

.box .tabscontents .tabcontent.show {
  display: block;
}

.box .logisbg {
  background: white;
  padding-bottom: 100rpx;
}

.box .tabscontents .tabcontent.hidden {
  display: none;
}

/* 运单详情 */

.sheetDetail {
  background: #fff;
}

.sheetDetail .li {
  padding: 16rpx 50rpx;
  border-bottom: 1px solid #e5e5e5;
  font-size: 28rpx;
}

.sheetDetail .li:last-child {
  border: none;
}

.sheetCity {
  display: flex;
  justify-content: center;
  padding: 6rpx 0;
  font-size: 48rpx;
}

.cityArrow {
  width: 44rpx;
  height: 12rpx;
  margin: 22rpx 26rpx;
  background-repeat: no-repeat;
  background-size: 100% 100%;
  background-image: url(data:image/png;base64,);
}

.sheetDetail .li .customer {
  font-size: 24rpx;
}

.sheetDetail .li .customer span {
  margin: 0 8rpx 0 0;
}

.sheetDetail .li.tables {
  display: flex;
  position: relative;
}

.sheetDetail .li.tables .tableright {
  position: absolute;
  left: 56%;
}

.sheetDetail .li.tables .line {
  width: 3rpx;
  height: 100%;
  background: #e5e5e5;
  position: absolute;
  z-index: 100;
  top: 0rpx;
  left: 50%;
  margin-left: -1rpx;
}

.sheetDetail .li.tables .table {
  width: 372rpx;
}

.sheetDetail .li.cost span:nth-child(even) {
  margin: 0 20rpx 0 0;
}

.sheetDetail .li .staffName {
  margin: 0 8rpx 0 0;
}

.costtext {
  margin: 0 30rpx 0 0;
}

/* 物流详情 */

.logisticsDetail {
  padding: 40rpx 0 32rpx 0rpx;
  background: #fff;
  display: none;
  margin: 0 0 0 70rpx;
}

.logisticsDetail.show {
  display: block;
  position: relative;
}

/* .logisticsDetail .li{
    margin:0 0 46rpx 0;
} */

.logisticsDetail .li .logsiticsPath .mess {
  color: #7a7a7a;
  font-size: 24rpx;
  margin: 0 0 10rpx 0;
  line-height: 40rpx;
}

.logisticsDetail .li .logsiticsPath .time {
  color: #afafaf;
  font-size: 22rpx;
}

.logisticsDetail .li {
  position: relative;
  display: flex;
  padding: 0 0 0 38rpx;
  width: 85%;
  flex-wrap: wrap;
}

.logisticsDetail .line {
  content: "";
  position: absolute;
  z-index: 1;
  pointer-events: none;
  background-color: #e5e5e5;
  width: 3rpx;
  left: 0;
  top: 60rpx;
  bottom: -56rpx;
  height: 100%;
}

.logisticsDetail  .line.noline {
  display: none;
}

.grayIcon {
  position: absolute;
  z-index: 100;
  left: -8rpx;
  top: 10rpx;
  width: 18rpx;
  height: 18rpx;
  background-repeat: no-repeat;
  background-size: 100% 100%;
  background-image: url(data:image/png;base64,);
  display: inline-block;
}

.grayIcon.firstIcon {
  background-image: url(data:image/png;base64,);
}

.cancel {
  background: #fff;
  margin: 16rpx 0 70rpx 526rpx;
  font-size: 28rpx;
  color: #4a4a4a;
  border-radius: 100rpx;
  width: 180rpx;
  height: 60rpx;
  border: 3rpx solid #e6e6e6;
  text-align: center;
  line-height: 58rpx;
  display: none;
}

.cancel.show {
  display: block;
}
.bottombackground{
  background-color: #F7F7F7;
  height: 54rpx;
}
.logistics-No {
  font-size: 40rpx;
  text-align: center;
  display: none;
  line-height: 150rpx;
  background: #fff;
}

.logistics-No.show {
  display: block;
}

.footer-nav{
  position: fixed;
  bottom: 0;
  width: 100%;
  display: flex;
  background: #ffffff;
  align-items: center;
  justify-content: center;
  box-shadow:0 0 10rpx 0 rgba(155,143,143,0.6);
  z-index: 9999;

}
.footer-nav>view{
  width: 50%;
  text-align: center;
  font-size:28rpx;
}
.footer-nav image{
  display: block;
  margin: 0 auto;
  width: 55rpx;
  height: 55rpx;
}
.share-btn{
  display: inline-block;
  width:120rpx;
  height:44rpx;
  line-height: 44rpx;
  color: #E83F57;
  border: 1rpx solid #E83F57;
  border-radius: 20rpx;
  font-size: 24rpx;
  position: fixed;
  right: 30rpx;
  bottom: 110rpx;
}