/*选择器*/

.selector {
  width: 100%;
  position: fixed;
  bottom: 0;
  left: 0;
  z-index: 999999;
}

.selector .inner_box {
  position: relative;
  z-index: 999999;
}

.selector .header_box {
  padding: 12rpx 0;
  overflow: hidden;
  box-sizing: border-box;
  background: #fff;
  display: flex;
  justify-content: center;
  border-radius: 10rpx;
  flex-direction: column;
  margin-top: 20rpx;
}

.line {
  margin-top: 20rpx;
  width: 100%;
  height: 1rpx;
  border-bottom: 1px solid #e9e9e9;
}

.selector .selected {
  height: 80rpx;
}

.selector .picker_box {
  /* width: 100%; */
  background: #fff;
  border-radius: 10rpx;
}

.picktitle {
  font-size: 35rpx;
  padding-top: 20rpx;
  padding-bottom: 20rpx;
  text-align: center;
}

.picktitle1 {
  font-size: 40rpx;
  padding-top: 20rpx;
  padding-bottom: 20rpx;
  text-align: center;
}

.selector .picker_box1 {
  /* width: 100%; */
  background: #fff;
  margin-top: 25rpx;
  border-radius: 10rpx;
  display: flex;
  justify-content: center;
  flex-direction: column;
}

.selector .picker_box picker-view-column {
  text-align: center;
}

.selector .picker_box picker-view-column view {
  line-height: 90rpx;
}

.selector .layer {
  width: 100%;
  height: 100%;
  position: fixed;
  left: 0;
  top: 0;
  z-index: 10;
  background: rgba(0, 0, 0, 0.5);
}

/*动画效果  */

@keyframes layui-m-anim-up {
  0% {
    opacity: 0;
    -webkit-transform: translateY(800px);
    transform: translateY(800px);
  }

  100% {
    opacity: 1;
    -webkit-transform: translateY(0);
    transform: translateY(0);
  }
}

.layui-m-anim-up {
  -webkit-animation-name: layui-m-anim-up;
  animation-name: layui-m-anim-up;
}

.inner_box {
  pointer-events: auto;
  -webkit-overflow-scrolling: touch;
  -webkit-animation-fill-mode: both;
  animation-fill-mode: both;
  -webkit-animation-duration: 0.3s;
  animation-duration: 0.3s;
  margin: 25rpx;
}


