/*选择器*/

.selector {
  width: 100%;
  position: fixed;
  bottom: 0;
  left: 0;
  z-index: 999999;
}

.selector .inner_box {
  position: relative;
  z-index: 999999;
}

.selector .header_box {
  padding: 25rpx 0;
  overflow: hidden;
  box-sizing: border-box;
  background: #fbf9fe;
}

.selector .header_box .item {
  float: left;
  font-size: 34rpx;
  color: #586c94;
  box-sizing: border-box;
}

.selector .header_box .l {
  width: 25%;
  padding-left: 25rpx;
}

.selector .header_box .c {
  width: 50%;
  text-align: center;
}

.selector .header_box .r {
  width: 25%;
  text-align: right;
  padding-right: 25rpx;
}

.selector .selected {
  height: 90rpx;
}

.selector .picker_box {
  /* width: 100%; */
  height: 470rpx;
  background: #fff;
  padding-top: 70rpx;
  padding-left: 20rpx;
  padding-right: 20rpx;
  padding-bottom: 30rpx;
}

.selector .picker_box picker-view-column {
  text-align: center;
}

.selector .picker_box picker-view-column view {
  line-height: 90rpx;
}

.selector .layer {
  width: 100%;
  height: 100%;
  position: fixed;
  left: 0;
  top: 0;
  z-index: 10;
  background: rgba(0, 0, 0, 0.5);
}

/*动画效果  */

@keyframes layui-m-anim-up {
  0% {
    opacity: 0;
    -webkit-transform: translateY(800px);
    transform: translateY(800px);
  }

  100% {
    opacity: 1;
    -webkit-transform: translateY(0);
    transform: translateY(0);
  }
}

.layui-m-anim-up {
  -webkit-animation-name: layui-m-anim-up;
  animation-name: layui-m-anim-up;
}

.inner_box {
  pointer-events: auto;
  -webkit-overflow-scrolling: touch;
  -webkit-animation-fill-mode: both;
  animation-fill-mode: both;
  -webkit-animation-duration: 0.3s;
  animation-duration: 0.3s;
}

